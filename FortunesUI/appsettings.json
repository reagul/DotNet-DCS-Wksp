{
  "spring": {
    "application": {
      "name": "FortunesUI"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Service": {
    "Url": "http://FortunesService/"
  },
  "eureka": {
    "client": {
